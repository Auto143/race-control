<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:RaceControl.Client"
             x:Class="RaceControl.Client.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>
    </Application.Styles>
    
    <Application.Resources>
        <FontFamily x:Key="IconFont">/Assets/Fonts/FA/FontAwesome6Free-Solid-900.otf#Font Awesome 6 Free</FontFamily>
    </Application.Resources>
</Application>
